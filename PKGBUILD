pkgname = ottosei
pkgver = 1.0
pkgrel = 1
pkgdesc = "ottoseiのテストパッケージ"
アーチ=（「任意」）
url = " https://github.com/FascodeNet/ottosei "
ライセンス=（' sushi were '）
depends =（' sudo '）
makedepends =（' git '）
source =（" $ {url} / archive / v $ {pkgver} .zip "）
md5sums =（'スキップ'）
_gitname = ' ottosei '
package() {
git clone https://github.com/YYTU-Aqua/ottosei.git
cd "${_gitname}" && cp ottosei /usr/bin/ottosei
cd -r usr/share/serene/ottosei /usr/share/ottosei
}

